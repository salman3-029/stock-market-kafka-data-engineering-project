   "source": [
    "s3_resource = boto3.resource('s3',aws_access_key_id=access_key, \n
    aws_secret_access_key=secret,)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "0f135e81",
   "metadata": {},
   "outputs": [],
   "source": [
    "for count, i in enumerate(consumer): \n
    csv_buffer = StringIO() \n
    csv_buffer.write(str(i.value)) \n
    filename = "stock_market_{}.json".format(count) \n
    s3_resource.Object("kafka-stock-market-data-salman",filename).put(Body=csv_buffer.getvalue())"
   ]
